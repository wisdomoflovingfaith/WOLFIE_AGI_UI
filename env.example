# WOLFIE AGI UI Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_BASE_URL=http://localhost:5000
VITE_API_TIMEOUT=30000

# OpenAI Configuration (for AI agent responses)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration (for database)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false

# Feature Flags
VITE_ENABLE_VOICE=true
VITE_ENABLE_RECORDING=true
VITE_ENABLE_NOTIFICATIONS=true

# Channel System Configuration
VITE_DEFAULT_POLLING_INTERVAL=2100
VITE_MAX_CHANNEL_MEMBERS=10
VITE_MESSAGE_HISTORY_LIMIT=100

# Security Configuration
VITE_ENABLE_CORS=true
VITE_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5000